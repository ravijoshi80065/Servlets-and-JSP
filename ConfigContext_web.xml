<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>ConfigContext</display-name>
  <welcome-file-list>
    <welcome-file>ConfigContextIndex.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>InServ</servlet-name>
    <servlet-class>ConfigContextInsertServ</servlet-class>
    <init-param>
      <param-name>table</param-name>
      <param-value>studata</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>ShowServ</servlet-name>
    <servlet-class>ConfigContextSelectServ</servlet-class>
    <init-param>
      <param-name>table</param-name>
      <param-value>studata</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>InServ</servlet-name>
    <url-pattern>/InsertServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowServ</servlet-name>
    <url-pattern>/SelectServlet</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>driver</param-name>
    <param-value>com.mysql.jdbc.Driver</param-value>
  </context-param>
  <context-param>
    <param-name>DBurl</param-name>
    <param-value>jdbc:mysql://localhost:3306/prj1</param-value>
  </context-param>
  <context-param>
    <param-name>user</param-name>
    <param-value>root</param-value>
  </context-param>
  <context-param>
    <param-name>password</param-name>
    <param-value>root</param-value>
  </context-param>
</web-app>