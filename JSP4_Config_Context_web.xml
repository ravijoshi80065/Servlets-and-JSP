<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>JSP_Examples</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>JSP4_Config_Context_Servlet</servlet-name>
  <jsp-file>/JSP4_Config_Context.jsp</jsp-file>
  
  <init-param>
  <param-name>name</param-name>
  <param-value>Jack Sparrow</param-value>
  </init-param>
  
  <init-param>
  <param-name>age</param-name>
  <param-value>35</param-value>
  </init-param>
  </servlet>

<servlet-mapping>
<servlet-name>JSP4_Config_Context_Servlet</servlet-name>
<url-pattern>/JSP4_link</url-pattern>
</servlet-mapping>  
</web-app>